<script>
    function checkPermission() {

        const hostnameFromUrl = (url) => new URL(url).hostname;

        const url = (window.location != window.parent.location)
            ? document.referrer
            : document.location.href;

        // Can only be loaded as iframe
        if (window.location.href == document.referrer) return false

        console.log("referrer:", document.referrer)
        console.log("location.href:", document.location.href)

        // Check if parent host has permission
        return hostnameFromUrl(document.referrer) != hostnameFromUrl("<%= config.rootUrl %>") ? false : true
    }
</script>