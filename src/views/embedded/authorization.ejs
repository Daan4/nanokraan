// document.referrer

function hostnameFromUrl(url){
    return new URL(url).hostname
}

function checkPermission() {
    const url = (window.location.hostname != window.parent.location.hostname)
        ? window.parent.location.hostname
        : document.location.hostname;

    if (url != hostnameFromUrl("<%= config.rootUrl %>")) {
        return false
    } else {
        return true
    }
}